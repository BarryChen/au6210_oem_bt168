                      memset(&gPlayCtrl, 0, sizeof(gPlayCtrl));
                      gFsInfo.CurrFATSecNum = 0;
                      gFsInfo.CurrDirSecNum = 0;
                      gFsInfo.FileSum = OTP_MAX_SONG_NUM;
                      gFsInfo.FolderSum = 1;
                      gFsInfo.FolderNumInFolder = OTP_MAX_SONG_NUM;
                      gFsInfo.FileSumInFolder = OTP_MAX_SONG_NUM;
                      gFsInfo.ValidFolderSum = 1;
                      gPlayCtrl.File.FileType = FILE_TYPE_MP3;
                      gPlayCtrl.FolderEnable = FALSE;
                      gPlayCtrl.FileNum = 1;
                      gFsInitOK = 1;
                      return TRUE;
              }
              
              
              BOOL OtpFileOpen(WORD FileNum)
              {
                      if(FileNum > OTP_MAX_SONG_NUM)
                      {
                          return FALSE;
                      }
              
                      if(NULL == OtpFileList[FileNum - 1].StartDataPtr)
                      {
                          return FALSE;
                      }
              
                      if(0 == OtpFileList[FileNum - 1].DataLen)
                      {
                          return FALSE;
                      }
              
                      gOtpFile.CurrFileNum  = FileNum;
                      gSongInfo.CurPlayTime = 0;
                      gOtpFile.StartDataPtr = (BYTE CODE *)(OtpFileList[FileNum - 1].StartDataPtr);
                      //DBG(("gOtpFilePtr:%lx\n", (DWORD)gOtpFile.StartDataPtr));
                      gOtpFile.DataLen      = OtpFileList[FileNum - 1].DataLen;
                      gOtpFile.CurrDataPtr  = gOtpFile.StartDataPtr;
                      gOtpFile.RemainBytes  = gOtpFile.DataLen;
                      gPlayCtrl.File.Size = gOtpFile.DataLen;
                      gPlayCtrl.RepeatMode = REPEAT_ALL;
                      return TRUE;    
              }
              
              
              WORD OtpFileRead(WORD Addr, WORD Len)
C51 COMPILER V9.00   OTP_FS                                                                11/14/2015 16:49:44 PAGE 6   

              {
                      if(0 == gOtpFile.RemainBytes) //EOF!
                      {
                          return 0;
                      }
              
                      if(gOtpFile.RemainBytes < Len)
                      {
                          Len = gOtpFile.RemainBytes;
                      }
              
                      memcpy((BYTE XDATA *)Addr, gOtpFile.CurrDataPtr, Len);
                      //DBG(("OtpFileRead, %x, %x, %x, %bx, %bx, %bx, %bx\n", Addr, (WORD)gOtpFile.CurrDataPtr, Len, gOtpFile.C
             -urrDataPtr[0], gOtpFile.CurrDataPtr[1], XBYTE[Addr+0], XBYTE[Addr+1]));
                      gOtpFile.CurrDataPtr += Len; 
                      gOtpFile.RemainBytes -= Len;
                      //DBG(("\nPtr:%x, Bytes:%x\n", (WORD)gOtpFile.CurrDataPtr, gOtpFile.RemainBytes));
                      return Len;
              }
              
              
              BOOL OtpFileEOF(VOID)
              {
                      //DBG((">>OtpFileEOF(),RemainBytes:%x\n", gOtpFile.RemainBytes));
                      if(0 == gOtpFile.RemainBytes)
                      {
                              //DBG(("otp EOF!\n"));
                              return TRUE;
                      }
                      //DBG(("<<OtpFileEOF(),RemainBytes:%x\n", gOtpFile.RemainBytes));
                      return FALSE;
              }
              
              
              VOID OtpFileRewind(VOID)
              {
                      gOtpFile.CurrDataPtr  = gOtpFile.StartDataPtr;
                      gOtpFile.RemainBytes  = gOtpFile.DataLen;
              }
              
